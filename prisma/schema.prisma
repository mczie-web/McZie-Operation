generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ExecutionEvent {
  id        String   @id @default(cuid())
  type      String
  status    String
  traceId   String?
  payload   Json?
  createdAt DateTime @default(now())
}
